{
  "builds": [
    {
      "src": "nuxt.config.ts",
      "use": "@nuxtjs/vercel-builder"
    }
  ],
  "routes": [
    {
      "src": "/api/.*",
      "headers": {
        "cache-control": "s-maxage=10"
      }
    },
    {
      "src": "/_nuxt/.*",
      "headers": {
        "cache-control": "s-maxage=86400"
      }
    }
  ],
  "functions": {
    "server/api/**/*.js": {
      "maxDuration": 30
    }
  },
  "env": {
    "NUXT_PUBLIC_BASE_URL": "@nuxt-public-base-url",
    "SUPABASE_URL": "@supabase-url",
    "SUPABASE_KEY": "@supabase-key",
    "SUPABASE_SERVICE_KEY": "@supabase-service-key",
    "NUXT_PUBLIC_MIDTRANS_CLIENT_KEY": "@midtrans-client-key",
    "NUXT_API_SECRET": "@api-secret"
  }
}
